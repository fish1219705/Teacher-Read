@model IEnumerable<Teacher.Models.Student>

<a style="margin-bottom:10px;" href="/StudentPage/New">Add Student</a>

<h2>Student List:</h2>
@foreach (Teacher.Models.Student Student in Model)
{
    <div>
        <a href="/StudentPage/Show/@Student.StudentId">@Student.StudentFirstName @Student.StudentLastName</a>
    </div>
}