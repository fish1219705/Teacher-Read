@model IEnumerable<Teacher.Models.Course>

<a style="margin-bottom:10px;" href="/CoursePage/New">Add Course</a>

<h2>Course List:</h2>
@foreach (Teacher.Models.Course Course in Model)
{
    <div>
        <a href="/CoursePage/Show/@Course.CourseId">@Course.CourseCode @Course.CourseName</a>
    </div>
}
